{
    "name": "@formio/enterprise-builder",
    "version": "1.0.0-dev.1",
    "exports": {
        "./angular": {
            "import": "./angular/enterprise-builder.js",
            "require": "./angular/enterprise-builder.js",
            "types": "./angular/index.d.ts"
        }
    },
    "scripts": {
        "copy:package": "node copy-package.js",
        "build:angular": "cd angular && npm run build && cd .. && webpack --config config/webpack.angular.js",
        "build": "npm run build:angular && npm run copy:package",
        "publish": "npm run build && npm publish ./build --access public --tag=dev"
    },
    "devDependencies": {
        "@formio/js": "^5.0.0-rc.76",
        "copy-webpack-plugin": "^12.0.2",
        "javascript-obfuscator": "^4.1.1",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0",
        "webpack-obfuscator": "^3.5.1"
    }
}

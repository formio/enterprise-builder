{
    "name": "@formio/enterprise-builder",
    "version": "1.0.0-dev.2",
    "repository": "git@github.com:formio/enterprise-builder.git",
    "license": "MIT",
    "exports": {
        "./angular/package.json": {
            "default": "./angular/package.json"
        },
        "./angular": {
            "types": "./angular/index.d.ts",
            "esm2022": "./angular/esm2022/formio-enterprise-builder-angular.mjs",
            "esm": "./angular/esm2022/formio-enterprise-builder-angular.mjs",
            "default": "./angular/fesm2022/formio-enterprise-builder-angular.mjs"
        }
    },
    "scripts": {
        "clean": "rm -rf build && mkdir build",
        "build:angular": "cd angular/src && yarn --force && ng cache clean && npm run build",
        "copy:package": "cp package.json build/package.json",
        "copy:angular": "rsync -av angular/src/dist/enterprise-builder/ build/angular",
        "build": "npm run clean && npm run build:angular && npm run copy:package && npm run copy:angular",
        "release": "npm run build && npm publish ./build --access public --tag dev"
    }
}
